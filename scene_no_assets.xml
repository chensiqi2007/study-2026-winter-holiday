<mujoco model="z1_factory_primitives">
    <compiler angle="radian" autolimits="true"/>
    <option integrator="implicitfast" timestep="0.002"/>

    <visual>
        <rgba haze="0.15 0.25 0.35 1"/>
        <quality shadowsize="4096"/>
    </visual>

    <default>
        <default class="z1">
            <joint damping="5" frictionloss="0.5"/>
            <motor ctrlrange="-50 50"/>
            <geom type="capsule" size="0.035" rgba="0.9 0.9 0.95 1" material="metal"/>
        </default>
        <default class="visual_joint">
            <geom type="cylinder" size="0.04 0.04" rgba="0.2 0.2 0.2 1" fromto="0 0 -0.04 0 0 0.04"/>
